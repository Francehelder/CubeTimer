#!@PYTHON@


import os
import sys
import signal
import locale
import gettext

VERSION = '@VERSION@'
pkgdatadir = '@pkgdatadir@'
localedir = '@localedir@'

sys.path.insert(1, pkgdatadir)
signal.signal(signal.SIGINT, signal.SIG_DFL)
locale.bindtextdomain('cube-timer-gtk', localedir)
locale.textdomain('cube-timer-gtk')
gettext.install('cube-timer-gtk', localedir)

if __name__ == '__main__':
    import gi

    from gi.repository import Gio
    resource = Gio.Resource.load(os.path.join(pkgdatadir, 'cube-timer-gtk.gresource'))
    resource._register()

    from cube_timer_gtk import main
    sys.exit(main.main(VERSION))
